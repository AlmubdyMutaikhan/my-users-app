<!DOCTYPE html>
<html>
    <head>
        <title>Users index page</title>
        <style>
            td, th
            {
                border: 1px solid #262626;
                padding: 5px;
                text-align: center;
            }

            tr:nth-child(even)
            {
                background-color: #eee;
            }
        </style>
    </head>
    <body>
        <h1>Users</h1>
        <table>
            <tr>
                <th>FirstName</th>
                <th>LastName</th>
                <th>Email</th>
                <th>Password</th>
                <th>Age</th>
                <th>User id</th>
            </tr>
                <% for(let i = 0; i < users.length;i++) { %>
                    <tr>
                        <% for(let j = 0; j < keys.length;j++) { %>
                            <% if(keys[i] !== 'password') { %>
                            <td>
                                <%= users[i][keys[j]]%>
                            </td>
                            <% }%>
                        <% }%>
                    </tr>
                <% }%>
        </table>
    </body>
</html>